// 将键绑定放入此文件中以覆盖默认值
[
    {
        "key": "cmd+=",
        "command": "editor.action.reindentlines"
    },
    {
        "key": "alt+`",
        "command": "workbench.action.quickOpenTerm"
    },
    {
        "key": "ctrl+o",
        "command": "-lineBreakInsert",
        "when": "editorTextFocus && !editorReadonly"
    }
]